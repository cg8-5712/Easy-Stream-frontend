# ZLMediaKit 流媒体服务器配置
# ZLMediaKit server host (default: current page hostname)
VITE_ZLM_HOST=localhost

# ZLMediaKit HTTP API port
VITE_ZLM_PORT=8080

# ZLMediaKit application name
VITE_ZLM_APP=live

# Use HTTPS for ZLMediaKit (true/false)
VITE_ZLM_USE_SSL=false

# API 配置
# Backend API base URL
VITE_API_BASE_URL=/api/v1

# API request timeout (milliseconds)
VITE_API_TIMEOUT=10000
